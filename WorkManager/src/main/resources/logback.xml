<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE logback>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{yyyy/MM/dd HH:mm:ss} %-5level %c{20} %m%n</pattern>
		</encoder>
		<charset>Shift-JIS</charset>
	</appender>

	<root>
		<appender-ref ref="STDOUT" />
	</root>
</configuration>